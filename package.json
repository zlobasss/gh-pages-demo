{
  "name": "gh-pages-demo",
  "version": "0.1.0",
  "homepage": "https://zlobasss.github.io/gh-pages-demo",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "cra-template-typescript": "1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "json-server --watch ./src/server/db/index.js --routes ./src/server/routes.json --port 3001",
    "save-json-api": "node ./src/scripts/save-json-api.js",
    "build": "react-scripts build && npm run save-json-api",
    "predeploy": "rm -rf build && npm run build",
    "deploy": "gh-pages -d build"
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "json-server": "^0.17.3",
    "node-fs": "^0.1.7"
  }
}
